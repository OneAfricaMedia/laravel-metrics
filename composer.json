{
    "name": "oneafricamedia/laravel-metrics",
    "description": "Integrate Laravel with InfluxDB easily.",
    "keywords": [
        "laravel",
        "influxbd",
        "metrics"
    ],
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ringier OneAfricaMedia",
            "email": "oamdev@oneafricamedia.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.5"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the metrics:push command (>=6.0)."
    },
    "autoload": {
        "psr-4": {
            "Roam\\Core\\Metrics\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Roam\\Core\\Metrics\\MetricServiceProvider"
            ]
        }
    }
}
